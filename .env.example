# 데이터베이스
DATABASE_URL=postgresql://user:password@host:5432/database?sslmode=require

# 네이버 커머스 API
NAVER_COMMERCE_CLIENT_ID=your_commerce_client_id
NAVER_COMMERCE_CLIENT_SECRET=your_commerce_client_secret

# 네이버 쇼핑 검색 API
NAVER_SHOPPING_CLIENT_ID=your_shopping_client_id
NAVER_SHOPPING_CLIENT_SECRET=your_shopping_client_secret

# 네이버 검색광고 API
